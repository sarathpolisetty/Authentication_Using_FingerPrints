Here is our Arduino code to register finger prints to fingerprint sensor:
(one can test it by running it in arduino):
#include <Adafruit_Fingerprint.h>
#include <SoftwareSerial.h>
SoftwareSerial mySerial(2, 3);
Adafruit_Fingerprint finger = Adafruit_Fingerprint(&mySerial);
uint8_t id,tmp;
void setup()  
{
  Serial.begin(9600);
  while (!Serial);  // For Yun/Leo/Micro/Zero/...
  delay(100);
 // Serial.println("\n\nAdafruit Fingerprint sensor enrollment");
  finger.begin(57600);
  if (finger.verifyPassword()) {
   Serial.println("Found fingerprint sensor!");
   delay(10);
  } else {
    Serial.println("some error");
    
    while (1) { delay(1); }
  }
}

uint8_t readnumber(void) {
  uint8_t num = 0;
  
  while (num == 0) {
    while (! Serial.available());
    num = Serial.parseInt();
  }
  return num;
}

void loop()                    
{

 id=readnumber();
 
  if (id == 0) {    
     return;
  }

  
  while (!  getFingerprintEnroll() );
  }

uint8_t getFingerprintEnroll() {

  int p = -1;
  Serial.println("place your finger");
  
  while (p != FINGERPRINT_OK) {
    p = finger.getImage();
    switch (p) {
    case FINGERPRINT_OK:
      break;
    default:
      Serial.println("Unknown error");
      break;
    }
  }

  // OK success!

  p = finger.image2Tz(1);
  switch (p) {
    case FINGERPRINT_OK:
      break;
    default:
      Serial.println("Unknown error");
      return p;
  }
  
  Serial.println("Remove finger");
  p = 0;

  while (p != FINGERPRINT_NOFINGER) {
    p = finger.getImage();
  }
 // Serial.print("ID "); Serial.println(id);
  Serial.println("Place same finger again");
  p = -1;

  
 while (p != FINGERPRINT_OK) {
    p = finger.getImage();
    switch (p) {
    case FINGERPRINT_OK:
      break;
    default:
      Serial.println("Unknown error");
      break;
    }
  }
  // OK success!

  p = finger.image2Tz(2);
  switch (p) {
    case FINGERPRINT_OK:
      break;
    default:
      Serial.println("Unknown error");
      return p;
  }
  
  // OK converted!
 // Serial.print("Creating model for #");  
  //Serial.println(id);
  
  p = finger.createModel();
  if (p == FINGERPRINT_OK) {
    Serial.println("Prints matched!");
  }  else {
    Serial.println("Unknown error");
    return p;
  }   
  
 // Serial.print("ID "); Serial.println(id);
  p = finger.storeModel(id);
  if (p == FINGERPRINT_OK) {
    Serial.println("Stored!");
  } else {
    Serial.println("Unknown error");
    return p;
  }  
  Serial.println("0"); 
  exit(0);
  
}


One has to write Corresponding python programme to communicate serially with the arduino and to get the fingerprint templates to the local data base
Here is our programme:-


#!C:\Users\Sarath\PycharmProjects\mini_pro\venv\Scripts\python.exe
import serial, time
import cgi
import cgitb
import pymysql

#For database connection
form = cgi.FieldStorage()
db = pymysql.connect("localhost","sarath","Sarath@123","maps")
cursor = db.cursor()


first = form.getvalue('first')
last = form.getvalue('last')
roll = form.getvalue('roll')
phone = form.getvalue('phone')
email = form.getvalue('email')
id=roll
a=id[-1]
b=id[-2]
if b>='0' and b<='9':
    c=10*int(b)+int(a)
else:
    c=((ord(b)-97)*10)+100+int(a)
d=str(c)

ser = serial.Serial("COM3",9600)
time.sleep(2)
var = ser.readline()
if (var == b'some error\r\n'):
    r=1
elif (var == b'Found fingerprint sensor!\r\n'):
    r=0

idi = d
var = '1'
ser.write(bytes(idi.encode('ascii')))
while (var != b'0\r\n'):
    var = ser.readline()
